# Server Configuration
PORT=5000

# Supabase Configuration
PUBLIC_SUPABASE_URL="your_supabase_url"
SUPABASE_SERVICE_ROLE_KEY="your_supabase_service_role_key"

# Hyperfeedback Application URL (used by server and client-side Vite)
HF_APP_URL="http://localhost:3000"

# Base URL for the proxy service (e.g., yourdomain.com or proxy.yourdomain.com)
# The proxy will operate on subdomains like <pid>.p.<NEXT_PUBLIC_PROXY_BASE_URL>
NEXT_PUBLIC_PROXY_BASE_URL="localhost:5000"

# Vite specific alias (Vite will automatically expose VITE_ variables from .env)
VITE_HF_APP_URL=${HF_APP_URL}